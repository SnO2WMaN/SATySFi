use List

module ListTest = struct

  #[test]
  val fold-left-test =
    let input = [3, 1, 4, 1, 5, 9, 2] in
    let expected = 25 in
    let got = List.fold-left ( + ) 0 input in
    if got == expected then
      None
    else
      Some(`FAILED: fold-left-test (expected: `# ^ (arabic expected) ^ `, got: `# ^ (arabic got) ^ `)`)

  #[test]
  val abort-test =
    abort-with-message `FAILED: abort`

end
